namespace {:config('codebase.app_namespace')}\{$moduleName}\model;

use think\Model;
use think\Db;

class {:tableNameToModelName($tableName)} extends Model {
	{if condition="$pk"}
	// 设置数据库主键字段
	protected $pk = '{$pk}';
	{/if}

	{if condition="$trueTable"}
// 设置当前模型对应的完整数据表名称
	// protected $table = '{$trueTable}';
	{/if}
protected function initialize()
	{
		parent::initialize();
	    $this->db = Db::name('{$tableName}');
	}
	{include file="add" /}
	{include file="edit" /}
	{include file="del" /}
	{include file="delList" /}
	{include file="info" /}
	{include file="lists" /}
    
}	